{
    "name": "friendsofphp/pickle",
    "type": "application",
    "description": "Installer for PHP extension, supports pecl package or install from src tree directly",
    "homepage": "https://github.com/FriendsOfPHP/pickle",
    "license": "PHP-3.01",
    "authors": [
        {
            "name": "Pierre Joye",
            "email": "pierre.php@gmail.com",
            "homepage": "http://pecl.php.net/user/pajoye",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.4",
        "symfony/console": "~2.5",
        "justinrainbow/json-schema": "~1.1",
        "composer/composer": "~1.0-alpha8@dev"
    },
    "require-dev": {
        "atoum/atoum": "dev-extension@dev",
        "atoum/praspel-extension": "dev-master@dev",
            "hoa/core": "dev-master@dev",
            "hoa/visitor": "dev-master@dev",
            "hoa/router": "dev-master@dev",
            "hoa/iterator": "dev-master@dev",
            "hoa/string": "dev-master@dev",
            "hoa/realdom": "dev-master@dev",
            "hoa/stream": "dev-master@dev",
            "hoa/file": "dev-master@dev",
            "hoa/compiler": "dev-master@dev",
            "hoa/math": "dev-master@dev",
            "hoa/praspel": "dev-master@dev",
            "hoa/regex": "dev-master@dev",
            "hoa/dispatcher": "dev-master@dev",
            "hoa/console": "dev-master@dev",
        "behat/behat": "~3.0",
        "symfony/process": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Pickle\\": ["src/"]
        }
    },
    "bin": ["bin/pickle"]
}
